<!-- index.nunjucks -->
{% extends "layout.nunjucks" %}

{% block content %}
<article>
  <header>
    <h2>This is a post called {{title}}</h2>
    <p>Posted by <time datetime="{{published | date('YYYY-MM-DDTHH:mm:ss\\Z')}}">{{author.name}}</time> at {{published | date}}</p>
  </header>

  {{body}}
</article>
{% endblock %}
