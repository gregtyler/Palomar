<!doctype html>
<html lang="en"{% block htmlTag %}{% endblock %}>
<head>
  <meta charset="UTF-8">
  <title>{% block title %}{{config.siteTitle}}{% endblock %}</title>
  <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
  <link rel="stylesheet" type="text/css" href="/main.css?20161203">
  <link rel="shortcut icon" href="/favicon.png">
  {% block header %}
  {% endblock %}
</head>
<body>
  <header class="c-header o-flex__container">
    <h1 class="c-header__title o-flex__item">
      <a href="/" class="c-header__link o-link-inherit">{{config.siteTitle}}</a>
    </h1>
    <a href="https://twitter.com/gregtyler" class="c-header__link c-header__link--external o-link-inherit" target="_blank">Twitter</a>
    <a href="https://github.com/gregtyler" class="c-header__link c-header__link--external o-link-inherit" target="_blank">GitHub</a>
  </header>
  <main>
    {% block content %}{% endblock %}
  </main>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-34023507-2', 'auto');
    ga('send', 'pageview');
  </script>
</body>
</html>
